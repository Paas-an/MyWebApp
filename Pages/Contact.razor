@page "/contact"

<PageTitle>Contact</PageTitle>



<div class="container">
	<h1 class="text-center"> Socials </h1>
</div>

<div class="row">
  <div class="card-deck">

	  <div class="card text-center mt-3">
		 <div class="card-block">
			 <div class="card-title">
				 <h4></h4>
			 </div>
			 <div class="row justify-content-center align-items-center">
				 <div class="col ">
					 <div class="card-body " style="width: 15rem;">
					<img src=@githubLogoLink class="img-fluid w-50" alt="Responsive-image">
					</div>
				 </div>
				  <div class="col position-fixed:right">
					 <div class="card-body ">
					<button type="button" class="btn btn-dark align-content-center" @onclick="OpenNewTabGit" >Go to Github</button>
					</div>
				 </div>
			 </div>
		 </div>
	  </div>

	  <div class="card text-center mt-3">
		 <div class="card-block size">
			 <div class="card-title">
				 <h4></h4>
			 </div>
			 <div class="row justify-content-center align-items-center">
				 <div class="col ">
					 <div class="card-body " style="width: 15rem;">
					<img src=@emailLogoLink class="img-fluid w-50" alt="Responsive-image">
					</div>
				 </div>
				 <div class="col mt-10">
					 <div class="card-body ">
					<webapp.Services.CopyToClipboardButton Text="jolsenskogtro@gmail.com"></webapp.Services.CopyToClipboardButton>
					</div>
				 </div>
			 </div>
		 </div>
	  </div>

	  <div class="card text-center mt-3 ">
		 <div class="card-block">
			 <div class="card-title">
				 <h4></h4>
			 </div>
			 <div class="row justify-content-center align-items-center">
				 <div class="col ">
					 <div class="card-body " style="width: 15rem;">
						<img src=@linkedinLogoLink class="img-fluid w-50" alt="Responsive-image">
					</div>
				 </div>
				 <div class="col">
					 <div class="card-body ">
					<button type="button" class="btn btn-dark float-left" @onclick="OpenNewTabLinkedin" >Go to linkedin</button>
					</div>
				 </div>
			 </div>
		 </div>
	  </div>
	  
  </div>
</div>

@code{
	private string emailLogoLink = "data:image/svg+xml;base64,";
	private string githubLogoLink="data:image/svg+xml;base64,";
	private string linkedinLogoLink = "data:image/svg+xml;base64,";
	
	@inject IJSRuntime jsRuntime

	public async Task OpenNewTabGit(){
		string url = "https://github.com/Paas-an";
		await jsRuntime.InvokeAsync<object>("open", url , "_blank");
	}

	public async Task OpenNewTabLinkedin(){
		string url = "https://www.linkedin.com/in/jonas-skogtr%C3%B8-olsen-6a2397167/";
		await jsRuntime.InvokeAsync<object>("open", url , "_blank");
	}
}


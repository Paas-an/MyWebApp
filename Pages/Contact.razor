@page "/contact"

<PageTitle>Contact</PageTitle>


<div class="container  text-center " >
<div class="card-deck" >
<div class="row justify-content-center ">

  <div class="col-sm-2 ">
    <div class="card mt-5  ">
          <button type="button" class="btn btn-outline-success  align-content-center" @onclick="OpenNewTabGit" >
              <img src=@githubLogoLink class="card-img-top" alt="Responsive-image">
          </button>
       <div class="card-footer bg-success text-black">
           Go to my Github
       </div>
    </div>
  </div>
  <div class="col-sm-2">
    <div class="card mt-5">
       <ClipboardButton @TextToCopy="jolsenskogtro@gmail.com">
            <img src=@emailLogoLink class="card-img-top" alt="Responsive-image">
       </ClipboardButton>
       <div class="card-footer bg-danger text-black ">
        Copy Email to clippboard
       </div>
    </div>
  </div>
</div>

<div class="row justify-content-center ">
  <div class="col-sm-2 mt-5">
    <div class="card">
      <button type="button" class="btn btn-outline-primary border-primary " @onclick="OpenNewTabLinkedin" >
          <img src=@linkedinLogoLink class="card-img-top" alt="Responsive-image">
           </button>
           <div class="card-footer bg-primary text-black ">
           Go to my Linkedin
       </div>
      </div>
  </div>
  <div class="col-sm-2">
    <div class="card mt-5">
       <button type="button" class="btn  btn-outline-warning border-warning" @onclick="OpenNewTabDownloadCV" >
            <img src=@cvLogoLink class="card-img-top" alt="Responsive-image">
       </button>
       <div class="card-footer bg-warning text-black">
           Click to download CV
       </div>
    </div>
  </div> 
</div>
</div>
</div>
@code{
  @inject IJSRuntime jsRuntime

    private string emailLogoLink = "data:image/svg+xml;base64,";
	private string githubLogoLink="data:image/svg+xml;base64,";
	private string linkedinLogoLink = "data:image/svg+xml;base64,";
	private string cvLogoLink = "data:image/svg+xml;base64,";
    

	public async Task OpenNewTabGit(){
		string url = "https://github.com/Paas-an";
		await jsRuntime.InvokeAsync<object>("open", url , "_blank");
	}

	public async Task OpenNewTabLinkedin(){
		string url = "https://www.linkedin.com/in/jonas-skogtr%C3%B8-olsen-6a2397167/";
		await jsRuntime.InvokeAsync<object>("open", url , "_blank");
	}

	public async Task OpenNewTabDownloadCV(){
		string url = "https://drive.google.com/uc?export=download&id=1cRPwzaoWKH27ohFx1sXfqt1wmXRe_D7J";
		await jsRuntime.InvokeAsync<object>("open", url , "_blank");
	}

}

